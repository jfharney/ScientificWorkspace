[paths]

[data]

[options]
# GIT_REPO must be replaced with the path to the local working copy of the
# git repository
prefix = GIT_REPO

# path_append is added to sys.path.
path_append = %(prefix)s/django-fe/constellation/constellationfe

# tcp_connection tells constellation where to go for backend services
tcp_connection = tcp://techint-b117:5555

messaging_timeout = 20000
workspace_page = constellationfe/index.html
doi_page = constellationfe/doi.html

[request_parameters]

TAG_OID = tag_nid
TAG_RESOURCE_OID = resource_nid
TAG_TYPE = type
TAG_NAME = name
TAG_DESCRIPTION = description
TAG_PATH = path
TAG_USERID = user_id

FILE_PATH = path
FILE_OID = file_oid

JOB_ID = jid

[header_tokens]
