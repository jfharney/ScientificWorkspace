html
  body
    
   
      
    
    
    div.navbar
      div.navbar-inner 
        div.container-fluid
          div.top-nav.nav-collapse
            ul.nav
              li 
                a(href="../settings/#{uid}") #{uid}
              li
                a(href="../workspace/#{uid}") Workspace
              li
                a(href="../doi/#{uid}") DOI
              li
                form.navbar-search.pull-left
                  input(placeholder="Search this workspace",name="query",type="text").search-query.span2
               
                
                 
          div.btn-group.pull-right
            ul.nav
              a.btn.dropdown-toggle(data-toggle="dropdown",href="#")
                i.icon-user
                span.hidden-phone #{uid}
                span.caret
              ul.dropdown-menu
                li
                  a(href="#") Profile
                li.divider
                li
                  a(href="login.html") Logout
                li.divider
                li
                  a#j1s.user_dropdown_list(role="menuitem",tabindex="-1",href="#") 1qk
                li(role="presentation")
                  a#j1s.user_dropdown_list(role="menuitem",tabindex="-1",href="#") jamroz
                li(role="presentation")
                  a#8xo.user_dropdown_list(role="menuitem",tabindex="-1",href="#") 8xo
                li(role="presentation")
                  
        
    div.container-fluid
    
      div.row-fluid
        div.span12
          div
            ul.breadcrumb
              li
                a(href="#")
                  span#user_id_label #{uid}
                span.divider /
              li
                a(href="#") DOI
      div.row-fluid
        div.span12#user_info_space
     
      div.row-fluid
        div.span12
          div
            p.user_info
      
      div.row-fluid
      
        //left bar
        div.span2.main-menu-span 
          div.well.nav-collapse.sidebar-nav
            ul.nav.nav-tabs.nav-stacked.main-menu
              li.nav-header.hidden-tablet Search
              li
                a.ajax-link(href="#")
                  span.hidden-tablet Place holder 1
              li
                a.ajax-link(href="#")
                  span.hidden-tablet Place holder 2
              li
                a.ajax-link(href="#")
                  span.hidden-tablet Place holder 3
         
        
        
        
        //main content
        div.span8     
      
          div.row-fluid.sortable
            div.span12
              div.box.span12
                div.box-header.well
                  h2 Search parameters
                    i.icon-user
                  div.box-icon
                    a.btn.btn-minimize.btn-round(href="#")
                      i.icon-chevron-up
                    a.btn.btn-close.btn-round(href="#")
                      i.icon-remove
             
                div.box-content
                  //div.row-fluid
                    div.span12 
                      fieldset Search Parameters
                  
                  div.row-fluid
                    div.span12 
                  
                  div.row-fluid
                    div.span12 
                      div.span6
                        form
                          fieldset
                            div.control-group
                              label.control-label Text
                              div.controls
                                input.input-xlarge.focused#search_text(type="text",value="")
                            //div.control-group
                              label.control-label Tag
                              div.controls
                                input.input-xlarge.focused#focusedInput(type="text",value="")
                            div.control-group
                              label.control-label From
                              div.controls
                                input.input-xlarge.datepicker#date01(type="text",value="")
                            div.control-group
                              label.control-label To
                              div.controls
                                input.input-xlarge.datepicker#date02(type="text",value="")
                      div.span6
                        div.row-fluid
                          div.span12  
                            div.control-group
                              label.control-label Search Over:
                              div.controls
                                labels.checkbox.inline Users
                                  input#Users(type="checkbox",value="Users",checked) 
                                labels.checkbox.inline Groups
                                  input#Groups(type="checkbox",value="Groups",checked) 
                                labels.checkbox.inline Files
                                  input#Files(type="checkbox",value="Files",checked) 
                                labels.checkbox.inline Directories
                                  input#Directories(type="checkbox",value="Directories",checked) 
                                labels.checkbox.inline Jobs
                                  input#Jobs(type="checkbox",value="Jobs",checked) 
                                labels.checkbox.inline Apps
                                  input#Apps(type="checkbox",value="Apps",checked) 
                                labels.checkbox.inline Tags
                                  input#Tags(type="checkbox",value="Tags",checked) 
                                   
                                 
                        div.row-fluid
                          div.span12  
                            div Insert Facets here
                    div.span12
                      hr
                    div.span12
                      div.control-group
                        div.controls   
                          button#search_button.btn.btn-large Search
                      
                      
                   
                      
          div.row-fluid.sortable
            div.span12
              div.box.span12
                div.box-header.well
                  h2 Search results
                    i.icon-user
                  div.box-icon
                    a.btn.btn-minimize.btn-round(href="#")
                      i.icon-chevron-up
                    a.btn.btn-close.btn-round(href="#")
                      i.icon-remove
                div.box-content#results
                  div.row-fluid
                    div.span12 
                      fieldset Live search results
                      
                  
                  //div.row-fluid
                    div.span12 
                       div record 1
                       div Name: 
                       div Type
                  
                  //div.row-fluid
                    div.span12 
                       div record 2
                       div Name: 
                       div Type
                  
                  
                 
                 
                  
                 
      
            
